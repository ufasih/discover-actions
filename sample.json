{
  "variables": {
    "release": "{{env `RELEASE`}}"
  },
  "builders": [
    {
      "type": "amazon-{{user `release`}}-ebs",
      "access_key": "...",
      "secret_key": "...",
      "region": "us-east-1",
      "source_ami": "ami-fce3c696",
      "instance_type": "t2.micro",
      "ssh_username": "ubuntu",
      "ami_name": "packer {{timestamp}}"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "inline": "echo {{user `release`}}"
    }
  ],
  "error-cleanup-provisioner": {
    "type": "shell-local",
    "inline": ["echo 'rubber ducky'"]
  }
}